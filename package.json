{
  "name": "RunVolunteerMiniApp",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://m0nz0.github.io/test",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-regular-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^3.0.0",
    "bootstrap": "5.3.8",
    "dayjs": "^1.11.13",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.1",
    "react-router-bootstrap": "^0.26.3",
    "react-router-dom": "^7.8.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "env-cmd -f .env.development react-scripts start",
    "start:prod": "env-cmd -f .env.production react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "npm run build && npm run copy404 && gh-pages -d build",
    "copy404": "powershell -Command \"Copy-Item -Path build\\index.html -Destination build\\404.html -Force\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "copyfiles": "^2.4.1",
    "env-cmd": "^11.0.0",
    "gh-pages": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "4.9.5"
  }
}
